<result>
  <bean>
    <name>${name}</name>
    <path>${path}</path>
  </bean>
  <method>
    <name>${methodName}</name>
  </method>
  <#if result?exists>
  <result>${result}</result>
  </#if>
  <#if error?exists>
    <error>${error}</error>
  </#if>
</result>
